<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>beats, pixels, code, life | a blog by Ian Timourian</title>
 <link href="http://beatspixelscodelife.com/feed/" rel="self"/>
 <link href="http://beatspixelscodelife.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://beatspixelscodelife.com/</id>
 <author>
   <name>beats, pixels, code, life</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://beatspixelscodelife.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://beatspixelscodelife.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
